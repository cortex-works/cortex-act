{
  "name": "terminal-master",
  "displayName": "Terminal Master",
  "description": "Minimal VS Code extension to list terminals, run commands, and read output",
  "version": "2.0.0",
  "publisher": "terminal-master",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terminal-master.list",
        "title": "List All Terminals",
        "category": "Terminal Master"
      },
      {
        "command": "terminal-master.run",
        "title": "Run Command in Terminal",
        "category": "Terminal Master"
      },
      {
        "command": "terminal-master.read",
        "title": "Read Terminal Output",
        "category": "Terminal Master"
      },
      {
        "command": "terminal-master.runAndWait",
        "title": "Run Command and Wait for Output",
        "category": "Terminal Master"
      }
    ]
  },
  "scripts": {
  "vscode:prepublish": "npm run compile",
  "compile": "tsc -p ./",
  "watch": "tsc -watch -p ./",
  "clean": "rm -rf out terminal-master-*.tgz",
  "build:all": "npm run compile && cd mcp-server && npm run build",
  "package": "vsce package"
  },
  "devDependencies": {
    "@types/node": "^16.18.126",
    "@types/vscode": "^1.102.0",
    "@vscode/vsce": "^2.32.0",
    "typescript": "^4.9.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}
